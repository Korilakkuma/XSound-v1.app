{
  "name": "xsound-app",
  "version": "1.0.0",
  "engines": {
    "node": ">=16.0.0 <=18.16.x"
  },
  "description": "Web Music Application by XSound (Web Audio API Library)",
  "main": "./assets/app.js",
  "scripts": {
    "clean": "rm -f register.js sw.js ./assets/app.* ./assets/vendor.*",
    "lint:js": "eslint 'src/**/*.{ts,tsx}' 'mock/**/*.ts' '.storybook/*.js' jest.config.js postcss.config.js webpack.config.js",
    "lint:css": "stylelint 'src/**/*.css' && prettier --check 'src/**/*.css'",
    "lint:html": "markuplint index.html",
    "lint": "run-p lint:js lint:css lint:html",
    "fix:js": "eslint --fix 'src/**/*.{ts,tsx}' 'mock/**/*.ts' '.storybook/*.js' jest.config.js postcss.config.js webpack.config.js",
    "fix:css": "stylelint --fix 'src/**/*.css' && prettier --write 'src/**/*.css'",
    "fix:html": "markuplint --fix index.html",
    "fix": "run-p fix:js fix:css fix:html",
    "type:main": "tsc --noEmit",
    "type:sw": "tsc --noEmit --project tsconfig.sw.json",
    "type": "run-p type:main type:sw",
    "build": "npm run clean && cross-env NODE_ENV=production webpack --progress --mode production",
    "dev": "webpack-dev-server --progress",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:verbose": "jest --verbose",
    "test:detect": "jest --detectOpenHandles",
    "migrate:actions": "set-env-to-github_env",
    "storybook": "storybook dev --port 6006",
    "build-storybook": "storybook build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint --fix"
    ],
    "src/**/*.css": [
      "stylelint --fix",
      "prettier --write 'src/**/*.css'"
    ],
    "index.html": [
      "markuplint --fix"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Korilakkuma/XSound.app.git"
  },
  "keywords": [
    "XSound.app",
    "XSound",
    "Web Music",
    "Web Audio API",
    "Web MIDI API",
    "WebRTC"
  ],
  "author": "Korilakkuma <rilakkuma.san.xjapan@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Korilakkuma/XSound.app/issues"
  },
  "homepage": "https://xsound.app",
  "files": [
    "assets",
    "index.html",
    "LICENSE",
    "package.json",
    "README.md"
  ],
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "xsound": "^3.1.1"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^7.0.9",
    "@storybook/addon-essentials": "^7.0.9",
    "@storybook/addon-links": "^7.0.9",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/react": "^7.0.9",
    "@storybook/react-webpack5": "^7.0.9",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.1",
    "@types/react": "^18.2.5",
    "@types/react-dom": "^18.2.4",
    "@types/react-redux": "^7.1.25",
    "@types/react-router-redux": "^5.0.22",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "chromatic": "^6.17.4",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.3",
    "css-minimizer-webpack-plugin": "^5.0.0",
    "dotenv-webpack": "^8.0.1",
    "eslint": "^8.40.0",
    "eslint-cli": "^1.1.1",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "html-webpack-plugin": "^5.5.1",
    "husky": "4.3.8",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "lint-staged": "^13.2.2",
    "markuplint": "^2.11.4",
    "mini-css-extract-plugin": "^2.7.5",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.23",
    "postcss-easy-import": "^4.0.0",
    "postcss-loader": "^7.3.0",
    "prettier": "^2.8.8",
    "react-test-renderer": "^18.2.0",
    "set-env-to-github_env": "^1.2.1",
    "storybook": "^7.0.9",
    "stylelint": "^15.6.1",
    "stylelint-config-recess-order": "^4.0.0",
    "stylelint-config-standard": "^33.0.0",
    "terser-webpack-plugin": "^5.3.7",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.2",
    "typescript": "^4.7.4",
    "url-loader": "^4.1.1",
    "webpack": "^5.82.0",
    "webpack-cli": "^5.0.2",
    "webpack-dev-server": "^4.13.3"
  }
}
