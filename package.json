{
  "name": "xsound-app",
  "version": "1.0.0",
  "engines": {
    "node": ">=12.0.0 <=16.15.0"
  },
  "description": "Multifunctional Synthesizer by XSound (Web Audio API Library)",
  "main": "./assets/app.js",
  "scripts": {
    "clean": "rimraf register.js sw.js ./assets/app.* ./assets/vendor.*",
    "lint:js": "eslint 'src/**/*.{ts,tsx}'",
    "lint:css": "stylelint 'src/**/*.css'",
    "lint": "run-p lint:js lint:css",
    "fix:js": "eslint --fix 'src/**/*.{ts,tsx}'",
    "fix:css": "stylelint --fix 'src/**/*.css'",
    "fix": "run-p fix:js fix:css",
    "type": "tsc --noEmit",
    "build": "npm run clean && cross-env NODE_ENV=production webpack --progress --mode production",
    "dev": "webpack-dev-server --progress",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:verbose": "jest --verbose",
    "storybook": "start-storybook --port 6006",
    "build-storybook": "build-storybook"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint --fix",
      "tsc --esModuleInterop --noEmit --skipLibCheck --jsx react"
    ],
    "src/**/*.css": [
      "stylelint --fix"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Korilakkuma/XSound.app.git"
  },
  "keywords": [
    "XSound.app",
    "XSound",
    "Web Music",
    "Web Audio API",
    "Web MIDI API",
    "WebRTC"
  ],
  "author": "Korilakkuma <rilakkuma.san.xjapan@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Korilakkuma/XSound.app/issues"
  },
  "homepage": "https://xsound.app",
  "files": [
    "assets",
    "index.html",
    "LICENSE",
    "package.json",
    "README.md"
  ],
  "dependencies": {
    "history": "^5.3.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-redux": "^8.0.0",
    "react-router-dom": "^6.3.0",
    "react-router-redux": "^4.0.8",
    "redux": "^4.2.0",
    "xsound": "^3.0.0-rc"
  },
  "devDependencies": {
    "@linaria/babel-preset": "^3.0.0-beta.15",
    "@linaria/core": "^3.0.0-beta.15",
    "@linaria/react": "^3.0.0-beta.15",
    "@linaria/shaker": "^3.0.0-beta.15",
    "@storybook/addon-actions": "^6.5.4",
    "@storybook/addon-essentials": "^6.5.4",
    "@storybook/addon-links": "^6.5.4",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/builder-webpack5": "^6.5.4",
    "@storybook/manager-webpack5": "^6.5.4",
    "@storybook/react": "^6.5.4",
    "@testing-library/react": "^13.2.0",
    "@types/history": "^4.7.11",
    "@types/jest": "^27.5.1",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.5",
    "@types/react-redux": "^7.1.24",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-router-redux": "^5.0.21",
    "@types/react-test-renderer": "^18.0.0",
    "@types/service_worker_api": "0.0.9",
    "@types/whatwg-fetch": "0.0.33",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "chromatic": "^6.5.4",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^4.0.0",
    "dotenv-webpack": "^7.1.0",
    "eslint": "^8.16.0",
    "eslint-cli": "^1.1.1",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-n": "^15.2.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "eslint-plugin-standard": "^5.0.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "4.3.8",
    "jest": "^28.1.0",
    "jest-environment-jsdom": "^28.1.0",
    "lint-staged": "^12.4.1",
    "mini-css-extract-plugin": "^2.6.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "postcss-easy-import": "^4.0.0",
    "postcss-loader": "^7.0.0",
    "react-test-renderer": "^18.0.0",
    "rimraf": "^3.0.2",
    "stylelint": "^14.8.4",
    "stylelint-config-standard": "^25.0.0",
    "terser-webpack-plugin": "^5.3.1",
    "ts-jest": "^28.0.3",
    "ts-loader": "^9.3.0",
    "typescript": "^4.6.4",
    "url-loader": "^4.1.1",
    "webpack": "^5.72.1",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.9.0"
  }
}
