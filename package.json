{
  "name": "xsound-app",
  "version": "1.0.0",
  "engines": {
    "node": ">=12.0.0 <14.15.4"
  },
  "description": "Multifunctional Synthesizer by XSound (Web Audio API Library)",
  "main": "./assets/app.js",
  "scripts": {
    "clean": "rimraf register.js sw.js ./assets/app.* ./assets/vendor.*",
    "lint:js": "eslint 'src/**/*.{ts,tsx}'",
    "lint:css": "stylelint 'src/**/*.css'",
    "lint": "run-p lint:js lint:css",
    "fix:js": "eslint --fix 'src/**/*.{ts,tsx}'",
    "fix:css": "stylelint --fix 'src/**/*.css'",
    "fix": "run-p fix:js fix:css",
    "type": "tsc --noEmit",
    "build": "npm run clean && cross-env NODE_ENV=production webpack --progress --mode production",
    "dev": "webpack-dev-server --progress",
    "test": "jest --coverage",
    "storybook": "start-storybook -s . -p 6006",
    "build-storybook": "build-storybook"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint --fix",
      "tsc --noEmit --skipLibCheck --esModuleInterop --jsx react"
    ],
    "src/**/*.css": [
      "stylelint --fix"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Korilakkuma/XSound.app.git"
  },
  "keywords": [
    "XSound.app",
    "XSound",
    "Web Music",
    "Web Audio API",
    "Web MIDI API",
    "WebRTC"
  ],
  "author": "Korilakkuma <rilakkuma.san.xjapan@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Korilakkuma/XSound.app/issues"
  },
  "homepage": "https://xsound.app",
  "files": [
    "assets",
    "index.html",
    "LICENSE",
    "package.json",
    "README.md"
  ],
  "dependencies": {
    "connected-react-router": "^6.9.1",
    "history": "^5.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "react-router-dom": "^5.3.0",
    "redux": "^4.1.2",
    "xsound": "^3.0.0-alpha"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.3.12",
    "@storybook/addon-essentials": "^6.3.12",
    "@storybook/addon-links": "^6.3.12",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/react": "^6.3.12",
    "@testing-library/react": "^12.1.2",
    "@types/history": "^4.7.9",
    "@types/jest": "^27.0.3",
    "@types/react": "^17.0.36",
    "@types/react-dom": "^17.0.11",
    "@types/react-redux": "^7.1.20",
    "@types/react-router-dom": "^5.3.2",
    "@types/react-router-redux": "^5.0.20",
    "@types/react-test-renderer": "^17.0.1",
    "@types/service_worker_api": "0.0.9",
    "@types/whatwg-fetch": "0.0.33",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.5.1",
    "css-minimizer-webpack-plugin": "^3.1.4",
    "dotenv-webpack": "^7.0.3",
    "eslint": "^8.3.0",
    "eslint-cli": "^1.1.1",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.1",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-standard": "^5.0.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "4.3.8",
    "jest": "^27.3.1",
    "lint-staged": "^12.1.2",
    "mini-css-extract-plugin": "^2.4.5",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.11",
    "postcss-easy-import": "^3.0.0",
    "postcss-loader": "^6.2.0",
    "react-test-renderer": "^17.0.2",
    "rimraf": "^3.0.2",
    "stylelint": "^14.1.0",
    "stylelint-config-standard": "^24.0.0",
    "terser-webpack-plugin": "^5.2.5",
    "ts-jest": "^27.0.7",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.2",
    "url-loader": "^4.1.1",
    "webpack": "^5.64.2",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.5.0"
  }
}
