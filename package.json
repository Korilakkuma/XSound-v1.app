{
  "name": "xsound-app",
  "version": "1.0.0",
  "engines": {
    "node": ">=14.0.0 <=18.12.x"
  },
  "description": "Web Music Application by XSound (Web Audio API Library)",
  "main": "./assets/app.js",
  "scripts": {
    "clean": "rimraf register.js sw.js ./assets/app.* ./assets/vendor.*",
    "lint:js": "eslint 'src/**/*.{ts,tsx}' 'mock/**/*.ts'",
    "lint:css": "stylelint 'src/**/*.css'",
    "lint:html": "markuplint index.html",
    "lint": "run-p lint:js lint:css lint:html",
    "fix:js": "eslint --fix 'src/**/*.{ts,tsx}' 'mock/**/*.ts'",
    "fix:css": "stylelint --fix 'src/**/*.css'",
    "fix:html": "markuplint --fix index.html",
    "fix": "run-p fix:js fix:css fix:html",
    "type:main": "tsc --noEmit",
    "type:sw": "tsc --noEmit --project tsconfig.sw.json",
    "type": "run-p type:main type:sw",
    "build": "npm run clean && cross-env NODE_ENV=production webpack --progress --mode production",
    "dev": "webpack-dev-server --progress",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:verbose": "jest --verbose",
    "test:detect": "jest --detectOpenHandles",
    "migrate:actions": "set-env-to-github_env",
    "storybook": "start-storybook --port 6006",
    "build-storybook": "build-storybook"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint --fix"
    ],
    "src/**/*.css": [
      "stylelint --fix"
    ],
    "index.html": [
      "markuplint --fix"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Korilakkuma/XSound.app.git"
  },
  "keywords": [
    "XSound.app",
    "XSound",
    "Web Music",
    "Web Audio API",
    "Web MIDI API",
    "WebRTC"
  ],
  "author": "Korilakkuma <rilakkuma.san.xjapan@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Korilakkuma/XSound.app/issues"
  },
  "homepage": "https://xsound.app",
  "files": [
    "assets",
    "index.html",
    "LICENSE",
    "package.json",
    "README.md"
  ],
  "dependencies": {
    "history": "^5.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "react-router-redux": "^4.0.8",
    "redux": "^4.2.0",
    "xsound": "^3.0.15"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.5.15",
    "@storybook/addon-essentials": "^6.5.15",
    "@storybook/addon-links": "^6.5.15",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/builder-webpack5": "^6.5.15",
    "@storybook/manager-webpack5": "^6.5.15",
    "@storybook/react": "^6.5.15",
    "@testing-library/react": "^13.4.0",
    "@types/history": "^4.7.11",
    "@types/jest": "^29.2.5",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@types/react-redux": "^7.1.25",
    "@types/react-router-redux": "^5.0.21",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "chromatic": "^6.11.4",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.3",
    "css-minimizer-webpack-plugin": "^4.2.2",
    "dotenv-webpack": "^8.0.1",
    "eslint": "^8.31.0",
    "eslint-cli": "^1.1.1",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-n": "^15.6.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "4.3.8",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "lint-staged": "^13.1.0",
    "markuplint": "^2.11.4",
    "mini-css-extract-plugin": "^2.7.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.20",
    "postcss-easy-import": "^4.0.0",
    "postcss-loader": "^7.0.2",
    "react-test-renderer": "^18.2.0",
    "rimraf": "^3.0.2",
    "set-env-to-github_env": "^1.1.1",
    "stylelint": "^14.16.1",
    "stylelint-config-standard": "^29.0.0",
    "terser-webpack-plugin": "^5.3.6",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.4.2",
    "typescript": "^4.7.4",
    "url-loader": "^4.1.1",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1"
  }
}
