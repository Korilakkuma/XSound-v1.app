{
  "name": "xsound-app",
  "version": "1.0.0",
  "engines": {
    "node": ">=10.19.0 <12.0.0"
  },
  "description": "Multifunctional Synthesizer by XSound (Web Audio API Library)",
  "main": "./assets/app.js",
  "scripts": {
    "clean": "rm -rf register.js sw.js ./assets/app.* ./assets/vendor.*",
    "lint:js": "eslint src",
    "lint:css": "stylelint src",
    "lint": "run-p lint:js lint:css",
    "fix:js": "eslint --fix src",
    "fix:css": "stylelint --fix src",
    "fix": "run-p fix:js fix:css",
    "check": "tsc --noEmit",
    "build": "npm run clean && npm run check && NODE_ENV=production webpack --progress --mode production",
    "dev": "webpack-dev-server --progress"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint --fix"
    ],
    "src/**/*.css": [
      "stylelint --fix"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Korilakkuma/XSound.app.git"
  },
  "keywords": [
    "XSound.app",
    "XSound",
    "Web Music",
    "Web Audio API",
    "Web MIDI API",
    "WebRTC"
  ],
  "author": "Korilakkuma <rilakkuma.san.xjapan@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Korilakkuma/XSound.app/issues"
  },
  "homepage": "https://xsound.app/",
  "files": [
    "assets",
    "index.html",
    "LICENSE",
    "package.json",
    "README.md"
  ],
  "dependencies": {
    "connected-react-router": "^6.8.0",
    "history": "^4.10.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "xsound": "^2.20.0"
  },
  "devDependencies": {
    "@types/react": "^16.9.55",
    "@types/react-dom": "^16.9.9",
    "@types/service_worker_api": "0.0.9",
    "@types/whatwg-fetch": "0.0.33",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "css-loader": "^5.0.0",
    "eslint": "^7.12.1",
    "eslint-cli": "^1.1.1",
    "eslint-config-standard": "^16.0.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-standard": "^4.0.2",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.1",
    "mini-css-extract-plugin": "^1.2.1",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "postcss-easy-import": "^3.0.0",
    "postcss-loader": "^4.0.4",
    "stylelint": "^13.7.2",
    "stylelint-config-standard": "^20.0.0",
    "terser-webpack-plugin": "^5.0.3",
    "ts-loader": "^8.0.8",
    "typescript": "^4.0.5",
    "url-loader": "^4.1.1",
    "webpack": "^5.4.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  }
}
