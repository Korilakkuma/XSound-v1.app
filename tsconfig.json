{
  "compilerOptions": {
    "target": "esnext",
    "module": "commonjs",
    "declaration": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": false,
    "strictNullChecks": true,
    "strictPropertyInitialization": true,
    "lib": [
      "dom",
      "es2015",
      "webworker"
    ],
    "outDir": "./assets/",
    "jsx": "react"
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "node_modules",
    "assets/**/*"
  ]
}
